{
  "productName": "CrystalChocolate",
  "name": "crystalchocolate",
  "version": "0.1.0",
  "description": "An electron UI to show the amount of money held by the Bit&Nibs proyect.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js --disable-gpu",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=BitNibs --version-string.FileDescription=CE --version-string.ProductName=\"CrystalChocolate\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Proyecto Bit&Nibs / German Lugo",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "crypto": "^1.0.1",
    "ejs": "^2.6.1",
    "electron": "2.0.14",
    "electron-ejs": "^1.2.0",
    "got": "^9.3.2",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.3.13",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "passport-jwt": "^4.0.0",
    "qs": "^6.5.2",
    "superagent": "^4.1.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "electron-packager": "^12.2.0",
    "nodemon": "^1.18.6"
  }
}
